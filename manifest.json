{
    "name": "Goalkeeper",
    "description": "Extension to keep track of goals",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "./build/index.html"
    },
    "permissions": [
      "storage"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    }
  }